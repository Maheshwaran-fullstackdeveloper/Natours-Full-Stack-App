{
  "name": "natours",
  "version": "1.0.0",
  "description": "Tour booking application",
  "license": "ISC",
  "author": "Maheshwaran G",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npx nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "debug": "ndb server.js",
    "import-data": "node dev-data/data/import-dev-data.js --import",
    "delete-data": "node dev-data/data/import-dev-data.js --delete",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pug": "^3.0.3",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^20.3.1",
    "validator": "^13.15.26",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "ndb": "^1.1.5",
    "nodemon": "^3.1.10",
    "parcel-bundler": "^1.12.5"
  }
}
